{
  "name": "Urban Dictionary Tooltip",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Gets Urban Dictionary Meaning for a English word",
  "content_scripts": [
    {
      "js": ["contentscript.js"],
      "css": ["style.css"],
      "matches": ["<all_urls>"]
    }
  ],
  "web_accessible_resources": ["script.js", "style.css"],
  "permissions": ["activeTab", "storage", "declarativeContent"]
}
